{
  "name" : "DE_Copilot_DQ",
  "jobMode" : "SCRIPT",
  "description" : "Loads the Rules generated by the LLM and apply the Data quality against the incoming file.",
  "role" : "arn:aws:iam::566801649699:role/DE_Copilot_Role",
  "executionProperty" : {
    "maxConcurrentRuns" : 10
  },
  "command" : {
    "name" : "glueetl",
    "scriptLocation" : "s3://aws-glue-assets-566801649699-us-east-1/scripts/DE_Copilot_DQ.py",
    "pythonVersion" : "3"
  },
  "defaultArguments" : {
    "--enable-metrics" : "true",
    "--enable-spark-ui" : "true",
    "--spark-event-logs-path" : "s3://aws-glue-assets-566801649699-us-east-1/sparkHistoryLogs/",
    "--enable-job-insights" : "true",
    "--enable-observability-metrics" : "true",
    "--enable-glue-datacatalog" : "true",
    "--job-bookmark-option" : "job-bookmark-disable",
    "--job-language" : "python",
    "--TempDir" : "s3://aws-glue-assets-566801649699-us-east-1/temporary/"
  },
  "maxRetries" : 0,
  "allocatedCapacity" : 20,
  "timeout" : 480,
  "maxCapacity" : 20.0,
  "glueVersion" : "5.0",
  "numberOfWorkers" : 10,
  "workerType" : "G.2X",
  "executionClass" : "STANDARD",
  "sourceControlDetails" : {
    "provider" : "GITHUB",
    "repository" : "DE_Copilot",
    "branch" : "main",
    "folder" : "AWS/Glue"
  }
}